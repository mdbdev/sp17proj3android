<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.sumukhshiv.mdbsocials">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/custom_dark">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".UserArea" />
        <activity android:name=".NewSocialActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DetailScreenActivity" />
        <activity android:name=".InterestedMembersActivity"></activity>
    </application>

</manifest>